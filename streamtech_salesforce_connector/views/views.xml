<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="salesforce_setting_inherit">
            <field name="name">salesforce.connector.salesforce.settings.inherit</field>
            <field name="model">salesforce.connector</field>
            <field name="inherit_id" ref="res.config.settings.salesforce_settings" />
            <field name="arch" type="xml">
                <xpath expr="//div[hasclass('settings')]/div[@data-key='salesforce_connector']/div[@id='salesforce_Settings']" position="inside">
                    <h2>Salesforce Settings</h2> 
                </xpath>
            </field>
        </record>
    </data>
</odoo>
