<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="salesforce_setting_inherit">
            <field name="name">res.config.settings.salesforce.settings.inherit</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="res.config.settings.salesforce_settings" />
            <field name="arch" type="xml">
                <xpath expr="//div[hasclass('settings')]/div[@data-key='salesforce_connector']/div[@id='salesforce_Settings']" position="after">
                        <div id="salesforce_Yan" attrs="{'invisible':[('sf_salesforce_enable','=',False)]}">
                            <h2>Yan</h2>
                        </div>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
